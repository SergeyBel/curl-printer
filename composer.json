{
    "name": "sergey-bel/curl-printer",
    "description": "Print psr requests as curl command string",
    "authors": [
        {
            "name": "Sergey Belov"
        }
    ],
    "autoload": {
        "psr-4": {
            "CurlPrinter\\": "./src",
            "CurlPrinter\\Tests\\": "./tests"
        }
    },

    "require": {
        "php": ">=8.2",
        "psr/http-message": "^2.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3.73",
        "phpstan/phpstan": "^2.1",
        "guzzlehttp/guzzle": "^7.9"
    }
}
